{
  "name": "koa2-app-outline",
  "version": "1.0.0",
  "description": "A Simple Boilerplate for a Koa v2 App",
  "main": "server.js",
  "scripts": {
    "babel": "babel -w src -d build --s",
    "prestart": "babel src -d build --s",
    "start": "node ./build/server.js",
    "dev": "nodemon ./build/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": "6.2.2"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.11.4",
    "babel-polyfill": "^6.9.1",
    "handlebars": "^4.0.5",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-compress": "^2.0.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.0.1",
    "koa-send": "^3.2.0",
    "koa-static": "^2.0.0",
    "koa-views": "^5.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-es2015-spread"
    ]
  }
}
